<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="FluidWebInterfaces.Desktop.Views.DocumentationView">
    <Grid ColumnDefinitions="200, *" Background="#161d35">
        <TreeView Grid.Column="0">
            <TreeViewItem Header="Home" Foreground="#fff"/>
            <TreeViewItem Header="Binding Properties" Foreground="#fff"/>
            <TreeViewItem Header="Commands" Foreground="#fff">
                <TreeViewItem Header="Render HTML files" Foreground="#fff"/>
                <TreeViewItem Header="If Statements" Foreground="#fff"/>
            </TreeViewItem>
        </TreeView>
        <ContentControl Background="#fff" Grid.Column="1"/>
    </Grid>
</UserControl>
