<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.PreviewWith>
        <Border Padding="20">
            <!-- Add Controls for Previewer Here -->
            <CheckBox Classes="DefaultACheckBox"/>
        </Border>
    </Design.PreviewWith>

    <!-- Add Styles Here -->
    <Style Selector="CheckBox.DefaultACheckBox /template/ Border">
        <Setter Property="BorderBrush" Value="{StaticResource ActiveInstancesSolidColorBrush}"/>
    </Style>
    
    <Style Selector="CheckBox.DefaultBCheckBox /template/ Border">
        <Setter Property="BorderBrush" Value="{StaticResource MockEndpointsSolidColorBrush}"/>
    </Style>
    
</Styles>
